doctype html
html
  head
    meta(charset='utf-8')
    title eCommerce
    link(rel='stylesheet', href='/css/style.css')
    body
      h1 eCommerce
      nav
        ul
          li: a(href='/') Home
          li: a(href='/about') About
          li: a(href='/faq') FAQ
      each item in items
        ul
          li= item.name
          li= item.dimensions.l
          li= item.dimensions.w
          li= item.dimensions.h
          li= item.weight
          li= item.color
          li= item.quantity
          li= item.msrp
          li= item.percentOff
          li= item.cost()

